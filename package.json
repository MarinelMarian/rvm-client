{
	"name": "rvm-client",
	"version": "1.0.0",
	"description": "client side integration of RVM",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --configuration=local --o",
		"start:prod": "ng serve --configuration=production",
		"build:local": "ng build --configuration=local  && cp .htaccess dist/a/.htaccess",
		"build:dev": "ng build --prod --configuration=development  && cp .htaccess dist/a/.htaccess",
		"build:prod": "ng build --prod  && cp .htaccess dist/a/.htaccess",
		"pwa:build:local": "ng build --configuration=local && sw-precache --root=dist --config=precache-config.js",
		"pwa:build:dev": "ng build --prod --configuration=development && sw-precache --root=dist --config=precache-config.js",
		"pwa:build:prod": "ng build --prod && sw-precache --root=dist --config=precache-config.js",
		"test": "ng test",
		"e2e": "ng e2e",
		"generate": "ng generate",
		"compodoc:gen": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
		"compodoc:serve": "./node_modules/.bin/compodoc -s src/tsconfig.app.json",
		"compodoc": "npm run compodoc:gen && npm run compodoc:serve",
		"precommit": "lint-staged",
		"lint": "tslint --fix --config tslint.json --project tsconfig.json",
		"format:fix": "pretty-quick --staged"
	},
	"lint-staged": {
		"src/**/*.ts": [
			"tslint --fix --config tslint.json --project tsconfig.json",
			"git add"
		]
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^8.2.0",
		"@angular/cdk": "~8.0.2",
		"@angular/common": "^8.2.0",
		"@angular/compiler": "^8.2.0",
		"@angular/core": "^8.2.0",
		"@angular/forms": "^8.2.0",
		"@angular/http": "^7.2.15",
		"@angular/material": "^8.1.2",
		"@angular/platform-browser": "^8.2.0",
		"@angular/platform-browser-dynamic": "^8.2.0",
		"@angular/router": "^8.2.0",
		"@angular/service-worker": "^8.2.0",
		"@ngtools/webpack": "^8.2.0",
		"bootstrap": "^4.3.1",
		"core-js": "^3.1.4",
		"font-awesome": "^4.7.0",
		"jquery": "^3.4.1",
		"luxon": "^1.17.2",
		"ng2-table": "^1.3.2",
		"popper.js": "^1.15.0",
		"rxjs": "^6.5.2",
		"websocket-driver": "^0.7.3",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.800.6",
		"@angular/cli": "^8.2.0",
		"@angular/compiler-cli": "^8.2.0",
		"@angular/language-service": "^8.2.0",
		"@compodoc/compodoc": "^1.1.10",
		"@ng-bootstrap/ng-bootstrap": "5.0.0",
		"@types/jasmine": "^3.3.16",
		"@types/jasminewd2": "~2.0.6",
		"@types/jquery": "^3.3.30",
		"@types/luxon": "^1.15.2",
		"@types/node": "^12.6.8",
		"codelyzer": "~5.1.0",
		"husky": "^2.7.0",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "^4.2.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "^2.1.0",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"lint-staged": "^8.2.1",
		"node-sass": "^4.12.0",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.1",
		"protractor": "^5.4.2",
		"sw-precache-webpack-plugin": "^0.11.5",
		"ts-node": "~8.2.0",
		"tslint": "~5.17.0",
		"typescript": "3.4.4"
	}
}
